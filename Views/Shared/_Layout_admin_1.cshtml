<script>
    function addToCart(id, name, price, quantity) {
        var data = {
            id: id,
            name: name,
            price: price,
            quantity: quantity
        };
        $.post("/Product/OnPostAddToCart", data, function (response) {
            alert(response.message);
        });
    }
</script>
