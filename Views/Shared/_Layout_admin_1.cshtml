<script>
    $(document).ready(function () {
        $(".footer-block .title").on("click", function () {
            var n = window, t = "inner", i;
            "innerWidth" in window || (t = "client",
                n = document.documentElement || document.body);
            i = {
                width: n[t + "Width"],
                height: n[t + "Height"]
            };
            i.width < 769 && $(this).siblings(".list").slideToggle("slow")
        })
    })
</script>
