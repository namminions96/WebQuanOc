</script>
<script>
    function newsletter_subscribe(n) {
        var t = $("#subscribe-loading-progress"), i;
        t.show();
        i = {
            subscribe: n,
            email: $("#newsletter-email").val()
        };
        $.ajax({
            cache: !1,
            type: "POST",
            url: "/subscribenewsletter",
            data: i,
            success: function (n) {
                $("#newsletter-result-block").html(n.Result);
                n.Success ? ($("#newsletter-subscribe-block").hide(),
                    $("#newsletter-result-block").show()) : $("#newsletter-result-block").fadeIn("slow").delay(2e3).fadeOut("slow")
            },
            error: function () {
                alert("Failed to subscribe.")
            },
            complete: function () {
                t.hide()
            }
        })
    }
    $(document).ready(function () {
        $("#newsletter-subscribe-button").on("click", function () {
            newsletter_subscribe("true")
        });
        $("#newsletter-email").on("keydown", function (n) {
            if (n.keyCode == 13)
                return $("#newsletter-subscribe-button").trigger("click"),
                    !1
        })
    })
</script>
