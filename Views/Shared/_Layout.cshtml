<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Lightning Esports</title>
    @*    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />*@
    @*  <link rel="stylesheet" href="~/Web_Report.styles.css" asp-append-version="true" />*@
    <link href="~/js/aos/aos.css" rel="stylesheet">

    <link href="~/js/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/js/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/js/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/js/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/js/swiper/swiper-bundle.min.css" rel="stylesheet">
    <meta content="" name="description">
    <meta content="" name="keywords">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
